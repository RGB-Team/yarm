{
  "name": "@yarm/ui",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "./*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "imports": {
    "#yarm/ui/*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@yarm/eslint-config": "workspace:*",
    "@yarm/tailwind-config": "workspace:*",
    "@yarm/typescript-config": "workspace:*",
    "tailwindcss": "^3.4.17",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "next": ">= 15.1.0"
  }
}
